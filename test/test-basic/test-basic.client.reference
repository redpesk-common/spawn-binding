SEND-CALL basic/ping true
ON-REPLY 1:basic/ping: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  },
  "response":"pong=1"
}
SEND-CALL basic/distro {"action":"start"}
ON-REPLY 2:basic/distro: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  },
  "response":{
    "api":"basic",
    "sandbox":"sandbox-basic",
    "command":"distro",
  }
}
SEND-CALL basic/sync {"action":"start"}
ON-EVENT basic/distro:
{
  "jtype":"afb-event",
  "event":"basic/distro",
  "data":{
    "cmd":"distro",
    "status":{
      "exit":0
    },
    "stdout":[
      "test-basic.binder.reference",
      "test-basic.client.reference",
      "test-basic.json",
      "test-basic.sh"
    ]
  }
}
ON-REPLY 3:basic/sync: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  },
  "response":{
    "cmd":"sync",
    "status":{
      "exit":0
    },
    "stdout":[
      "test-basic.binder.reference",
      "test-basic.client.reference",
      "test-basic.json",
      "test-basic.sh"
    ]
  }
}
