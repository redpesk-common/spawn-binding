SEND-CALL ctl/ping true
ON-REPLY 1:ctl/ping: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  },
  "response":"pong=1"
}
SEND-CALL ctl/call true
ON-REPLY 2:ctl/call: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  }
}
SEND-CALL ctl/subcall true
ON-REPLY 3:ctl/subcall: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  },
  "response":{
    "cmd":"sync",
    "status":{
      "exit":0
    },
    "stdout":[
      "World!"
    ]
  }
}
SEND-CALL ctl/distro {"action":"start"}
ON-REPLY 4:ctl/distro: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  },
  "response":{
    "api":"ctl",
    "sandbox":"sandbox-basic",
    "command":"distro",
  }
}
SEND-CALL ctl/sync {"action":"start"}
ON-EVENT ctl/distro:
{
  "jtype":"afb-event",
  "event":"ctl/distro",
  "data":{
    "cmd":"distro",
    "status":{
      "exit":0
    },
    "stdout":[
      "Hello"
    ]
  }
}
ON-REPLY 5:ctl/sync: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  },
  "response":{
    "cmd":"sync",
    "status":{
      "exit":0
    },
    "stdout":[
      "World!"
    ]
  }
}
SEND-CALL ctl/exit true
ON-REPLY 6:ctl/exit: OK
{
  "jtype":"afb-reply",
  "request":{
    "status":"success",
    "code":0
  }
}
